<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/dp_5" />
            <gradient
                android:angle="270"
                android:centerColor="@color/white"
                android:centerY="0.75"
                android:endColor="@color/white"
                android:startColor="@color/white" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="@dimen/dp_5" />
                <gradient
                    android:angle="270"
                    android:centerColor="@color/color_F8F9F9"
                    android:centerY="0.75"
                    android:endColor="@color/color_F8F9F9"
                    android:startColor="@color/color_F8F9F9" />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/dp_5" />
                <gradient
                    android:angle="270"
                    android:centerColor="@color/color_10955B"
                    android:centerY="0.75"
                    android:endColor="@color/color_10955B"
                    android:startColor="@color/color_10955B" />
            </shape>
        </clip>
    </item>

</layer-list>