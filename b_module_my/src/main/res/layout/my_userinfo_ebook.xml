<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/com.moocxuetang"
    xmlns:app1="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="10dp">

    <include
        android:id="@+id/bar"
        layout="@layout/commonrs_item_e_book"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app1:layout_constraintLeft_toLeftOf="parent"
        app1:layout_constraintTop_toTopOf="parent" />


    <include
        android:id="@+id/incloudBottom"
        layout="@layout/my_include_item_myshare_bottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app1:layout_constraintLeft_toLeftOf="parent"
        app1:layout_constraintRight_toRightOf="parent"
        app1:layout_constraintTop_toBottomOf="@+id/bar"></include>

</androidx.constraintlayout.widget.ConstraintLayout>
